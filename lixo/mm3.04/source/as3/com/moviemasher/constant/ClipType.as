package com.moviemasher.constant
{
/** 
* Static class containing String constants for the seven supported values for type attributes in clip and media tags within the configuration.
*/
	public class ClipType
	{
/** 
* audio
*/
		public static const AUDIO:String = 'audio';
/** 
* effect
*/
		public static const EFFECT:String = 'effect';
/** 
* image
*/
		public static const IMAGE:String = 'image';
/** 
* mash
*/
		public static const MASH:String = 'mash';
/** 
* theme
*/
		public static const THEME:String = 'theme';
/** 
* transition
*/
		public static const TRANSITION:String = 'transition';
/** 
* video
*/
		public static const VIDEO:String = 'video';
	}
}
